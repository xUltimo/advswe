    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@auth0/angular-jwt": "^5.0.1",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@types/mongoose": "5.5.17",
    "babel-jest": "^26.6.3",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "core-js": "^3.8.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.10.13",
    "mongoose-gridfs": "1.2.48",
    "mongoose-type-email": "1.0.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
